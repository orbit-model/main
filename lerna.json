{
  "command": {
    "version": {
      "allowBranch": [
        "master"
      ],
      "message": "orbit-model release: %s"
    },
    "publish": {
      "graphType": "all"
    }
  },
  "ignoreChanges": [
    "/app/**",
    "**/test/**",
    "**/.*",
    "**/*.js",
    "**/*.md",
    "**/*.json"
  ],
  "packages": [
    "@orbit-model/*"
  ],
  "version": "0.0.1"
}
