{
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "develop"
      ],
      "message": "orbit-model release: %s"
    },
    "publish": {
      "graphType": "all"
    }
  },
  "useWorkspaces": true,
  "npmClient": "yarn",
  "ignoreChanges": [
    "/app/**",
    "**/test/**",
    "**/.*",
    "**/*.js",
    "**/*.md",
    "**/*.json"
  ],
  "packages": [
    "@orbit-model/*",
    "demo/*"
  ],
  "version": "0.0.2-beta.0"
}
